    "def mod_div(fun):\n",
    "    def inner(a,b):\n",
    "        if a > b:\n",
    "            a,b=b,a\n",
    "        fun(a,b)\n",
    "    return inner\n",
    "\n",
    "\n",
    "@mod_div\n",
    "def div(a,b):\n",
    "    print(a%b)\n",
    "    \n",
    "div(2,4)    "
   ]
  }
